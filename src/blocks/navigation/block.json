{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "renderkit/navigation",
    "version": "1.2.0",
    "title": "Navigation",
    "category": "renderkit",
    "icon": {
        "src": "<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='1.5' stroke-linecap='round' stroke-linejoin='round'><rect x='3' y='3' width='18' height='18' rx='2'/><line x1='3' y1='9' x2='21' y2='9'/><circle cx='7' cy='6' r='1'/><line x1='11' y1='6' x2='17' y2='6'/></svg>"
    },
    "description": "Premium rounded navigation bar with sticky option",
    "keywords": [
        "menu",
        "nav",
        "header",
        "navigation"
    ],
    "supports": {
        "html": false,
        "align": [
            "full",
            "wide"
        ]
    },
    "attributes": {
        "menuSlug": {
            "type": "string",
            "default": "renderkit-primary"
        },
        "showLogo": {
            "type": "boolean",
            "default": true
        },
        "logoUrl": {
            "type": "string",
            "default": ""
        },
        "siteName": {
            "type": "string",
            "default": ""
        },
        "sticky": {
            "type": "boolean",
            "default": true
        },
        "theme": {
            "type": "string",
            "default": "light",
            "enum": [
                "light",
                "dark"
            ]
        },
        "showCart": {
            "type": "boolean",
            "default": false
        }
    },
    "textdomain": "renderkit"
}