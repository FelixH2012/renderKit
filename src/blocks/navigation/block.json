{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "renderkit/navigation",
    "version": "1.2.0",
    "title": "Navigation",
    "category": "renderkit",
    "icon": "menu",
    "description": "Premium rounded navigation bar with sticky option",
    "keywords": [
        "menu",
        "nav",
        "header",
        "navigation"
    ],
    "supports": {
        "html": false,
        "align": [
            "full",
            "wide"
        ]
    },
    "attributes": {
        "menuSlug": {
            "type": "string",
            "default": "renderkit-primary"
        },
        "showLogo": {
            "type": "boolean",
            "default": true
        },
        "logoUrl": {
            "type": "string",
            "default": ""
        },
        "siteName": {
            "type": "string",
            "default": ""
        },
        "sticky": {
            "type": "boolean",
            "default": true
        },
        "theme": {
            "type": "string",
            "default": "light",
            "enum": [
                "light",
                "dark"
            ]
        },
        "showCart": {
            "type": "boolean",
            "default": false
        }
    },
    "textdomain": "renderkit",
    "render": "file:./render.php"
}