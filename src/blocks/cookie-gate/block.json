{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "renderkit/cookie-gate",
    "version": "1.0.0",
    "title": "Cookie Gate",
    "category": "renderkit",
    "description": "Only shows its content after a specific cookie consent.",
    "keywords": [
        "cookie",
        "consent",
        "gate"
    ],
    "icon": "lock",
    "textdomain": "renderkit",
    "supports": {
        "html": false,
        "align": [
            "wide",
            "full"
        ]
    },
    "attributes": {
        "requiredSetting": {
            "type": "string",
            "default": "analytics"
        },
        "consentVersion": {
            "type": "string",
            "default": "1"
        },
        "fallbackText": {
            "type": "string",
            "default": "Bitte akzeptiere die passenden Cookies, um diesen Inhalt zu sehen."
        },
        "previewAccepted": {
            "type": "boolean",
            "default": false
        }
    }
}
